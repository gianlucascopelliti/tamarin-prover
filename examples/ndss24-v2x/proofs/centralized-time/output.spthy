lemma exists_par_tv_5 [heuristic=I]:
  exists-trace
  "∃ tv #i. (SystemInitialized( tv ) @ #i) ∧ (tv = ('1'+'1'+'1'+'1'+'1'))"
/*
guarded formula characterizing all satisfying traces:
"∃ tv #i. (SystemInitialized( tv ) @ #i) ∧ (tv = ('1'+'1'+'1'+'1'+'1'))"
*/
simplify
solve( SystemInitialized( ('1'+'1'+'1'+'1'+'1') ) @ #i )
  case Init
  solve( TvTmp( ('1'+'1'+'1'+'1'+'1') ) ▶₁ #i )
    case increment_Tv
    solve( TvTmp( ('1'+'1'+'1'+'1') ) ▶₀ #vr )
      case increment_Tv
      solve( TvTmp( ('1'+'1'+'1') ) ▶₀ #vr.1 )
        case increment_Tv
        solve( TvTmp( ('1'+'1') ) ▶₀ #vr.2 )
          case increment_Tv
          solve( TvTmp( '1' ) ▶₀ #vr.3 )
            case init_parameters
            SOLVED // trace found
          qed
        qed
      qed
    qed
  qed
qed

