  GreaterEqualThan( (t_hb+tv), t ), GreaterEqualThan( (t+tv), t_hb ),
  ShouldNotRevoke( prl ), HeartbeatProcessed( <prl, t_hb>, t ),
  NewTime( 'TC', t_hb )
  ]->
