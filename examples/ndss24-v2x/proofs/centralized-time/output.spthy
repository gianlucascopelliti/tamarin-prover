theory RevocationCentralizedTime begin

// Function signature and definition of the equational theory E

builtins: multiset
functions: fst/1, pair/2, pk/1, sign/2, snd/1, true/0, verify/3
equations:
    fst(<x.1, x.2>) = x.1,
    snd(<x.1, x.2>) = x.2,
    verify(sign(x.1, x.2), x.1, pk(x.2)) = true



predicate: LessThan( x, y )<=>∃ z. (x+z) = y

predicate: GreaterThan( x, y )<=>∃ z. x = (y+z)

predicate: GreaterEqualThan( x, y )<=>(x = y) ∨ (∃ z. x = (y+z))

predicate: IsNotInList( x, y )<=>(((¬(x = y)) ∧ (¬(∃ a. (a+x) = y))) ∧ (¬(∃ b. (b+x) = y))) ∧
(¬(∃ c d. (c+d+x) = y))

predicate: IsInList( x, y )<=>(((x = y) ∨ (∃ a. (a+x) = y)) ∨ (∃ b. (b+x) = y)) ∨
(∃ c d. (c+d+x) = y)

restriction Equality:
  "∀ x y #i. (Eq( x, y ) @ #i) ⇒ (x = y)"
  // safety formula

restriction OnlyOnce:
  "∀ x #i #j. ((OnlyOnce( x ) @ #i) ∧ (OnlyOnce( x ) @ #j)) ⇒ (#i = #j)"
  // safety formula

restriction GreaterEqualThan:
  "∀ x y #i.
    (GreaterEqualThan( x, y ) @ #i) ⇒ ((x = y) ∨ (∃ z. x = (y+z)))"

restriction IsNotRevoked:
  "∀ #i.
    (IsNotRevoked( ) @ #i) ⇒ (¬(∃ t #j. (Revoked( t ) @ #j) ∧ (#j < #i)))"
  // safety formula

restriction ShouldRevoke:
  "∀ prl #i.
    (ShouldRevoke( prl ) @ #i) ⇒
    (∃ ps #j.
      ((NewPseudonym( ps ) @ #j) ∧ (#j < #i)) ∧
      ((((ps = prl) ∨ (∃ a. (a+ps) = prl)) ∨ (∃ b. (b+ps) = prl)) ∨
       (∃ c d. (c+d+ps) = prl)))"

restriction ShouldNotRevoke:
  "∀ prl #i.
    (ShouldNotRevoke( prl ) @ #i) ⇒
    (¬(∃ ps #j.
        ((NewPseudonym( ps ) @ #j) ∧ (#j < #i)) ∧
        ((((ps = prl) ∨ (∃ a. (a+ps) = prl)) ∨ (∃ b. (b+ps) = prl)) ∨
         (∃ c d. (c+d+ps) = prl))))"
  // safety formula

restriction IsLatestTime:
  "∀ x t #i.
    (IsLatestTime( x, t ) @ #i) ⇒
    (¬(∃ t2 #j. ((NewTime( x, t2 ) @ #j) ∧ (#j < #i)) ∧ (∃ z. t2 = (t+z))))"
  // safety formula

restriction IsLatestPRL:
  "∀ cnt #i.
    (IsLatestPRL( cnt ) @ #i) ⇒
    (¬(∃ prl cnt2 #j.
        ((PRLUpdated( prl, cnt2 ) @ #j) ∧ (#j < #i)) ∧ (∃ z. cnt2 = (cnt+z))))"
  // safety formula

rule (modulo E) init_parameters:
   [ ] --[ OnlyOnce( 'init_parameters' ) ]-> [ TvTmp( '1' ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) increment_Tv:
   [ TvTmp( tv ) ] --> [ TvTmp( ('1'+tv) ) ]

  // loop breaker: [0]
  /* has exactly the trivial AC variant */

rule (modulo E) Init:
   [ Fr( ~ltk ), TvTmp( tv ) ]
  --[
  OnlyOnce( 'Init' ), SystemInitialized( tv ), NewTime( 'TC', '1' ),
  NewTime( 'RA', '1' ), PRLUpdated( '0', '1' )
  ]->
   [
   !Time( 'TC', '1' ), !Time( 'RA', '1' ), !Parameters( tv ),
   !Ltk( 'RA', ~ltk ), !Pk( 'RA', pk(~ltk) ), !PRL( '0', '1' ),
   Out( pk(~ltk) )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) process_message:
   [
   In( <<m, t_v2v>, signature> ), !Parameters( tv ), !Pseudonym( ps ),
   !Time( 'RA', t )
   ]
  --[
  Eq( verify(signature, <m, t_v2v>, ps), true ),
  GreaterEqualThan( (t_v2v+tv), t ), MessageAccepted( <m, t_v2v>, ps, t )
  ]->
   [ ]

  /*
  rule (modulo AC) process_message:
     [
     In( <<m, t_v2v>, signature> ), !Parameters( tv ), !Pseudonym( ps ),
     !Time( 'RA', t )
     ]
    --[
    Eq( z, true ), GreaterEqualThan( (t_v2v+tv), t ),
    MessageAccepted( <m, t_v2v>, ps, t )
    ]->
     [ ]
    variants (modulo AC)
    1. m     = m.10
       ps    = ps.11
       signature
             = signature.12
       t_v2v = t_v2v.14
       z     = verify(signature.12, <m.10, t_v2v.14>, ps.11)
    
    2. m     = m.12
       ps    = pk(x.21)
       signature
             = sign(<m.12, t_v2v.16>, x.21)
       t_v2v = t_v2v.16
       z     = true
  */

rule (modulo E) TC_get_pseudonym:
   [ !Time( 'TC', t ), Fr( ~ps_key ) ]
  --[ IsNotRevoked( ), IsLatestTime( 'TC', t ), NewPseudonym( pk(~ps_key) )
  ]->
   [
   Out( pk(~ps_key) ), !Pseudonym( pk(~ps_key) ),
   !Ltk( pk(~ps_key), ~ps_key )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) TC_sign_message:
   [ !Time( 'TC', t ), !Pseudonym( ps ), !Ltk( ps, ps_key ), Fr( ~m ) ]
  --[ IsNotRevoked( ), IsLatestTime( 'TC', t ), Signed( <~m, t>, ps ) ]->
   [ Out( <<~m, t>, sign(<~m, t>, ps_key)> ) ]

  /* has exactly the trivial AC variant */

