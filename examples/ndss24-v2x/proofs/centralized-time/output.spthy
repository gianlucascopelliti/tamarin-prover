lemma revocation_possible [heuristic=o "oracle.py"]:
  exists-trace "∃ t #i. Revoked( t ) @ #i"
/*
guarded formula characterizing all satisfying traces:
"∃ t #i. (Revoked( t ) @ #i)"
*/
simplify
solve( !Pk( 'RA', pk(x) ) ▶₂ #i )
  case Init_case_1
  solve( !Parameters( tv ) ▶₁ #i )
    case Init
    solve( !Time( 'TC', t ) ▶₀ #i )
      case Init
      solve( !Time( 'TC', t ) ▶₀ #j )
        case Init
        solve( !KU( sign(<prl, t_hb>, ~ltk) ) @ #vk.4 )
          case RA_generate_heartbeat_case_2
          solve( !KU( pk(~ps_key) ) @ #vk.6 )
            case TC_get_pseudonym
            solve( (∃ a. ((a+pk(~ps_key.1)) = (prl+pk(~ps_key))))  ∥
                   (∃ b. ((b+pk(~ps_key.1)) = (prl+pk(~ps_key))))  ∥
                   (∃ c d. ((c+d+pk(~ps_key.1)) = (prl+pk(~ps_key)))) )
              case case_1_case_2
              solve( !PRL( prl, cnt ) ▶₂ #vr.4 )
                case Init
                SOLVED // trace found
              qed
            qed
          qed
