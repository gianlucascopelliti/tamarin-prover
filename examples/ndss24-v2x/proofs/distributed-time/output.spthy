                                             heuristic=o "oracle.py"]:
  all-traces
  "∀ m t_v2v ps t #i.
    (MessageAccepted( <m, t_v2v>, ps, t ) @ #i) ⇒
    (∃ tv #j.
      ((SystemInitialized( tv ) @ #j) ∧ (#j < #i)) ∧
      (¬(∃ ps2 t_rev #k.
          ((RevocationIssued( ps2, t_rev ) @ #k) ∧ (#k < #i)) ∧
          (∃ z. t_v2v = (t_rev+tv+z)))))"
/*
